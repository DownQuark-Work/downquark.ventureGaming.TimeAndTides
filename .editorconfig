# This file is for unifying the coding style for different editors and IDEs.
# EditorConfig is AweSome: http://editorconfig.org/

root = true

[*]
charset = UTF-8
end_of_line = LF
indent_brace_style = Allman
indent_size = 2
indent_style = space
insert_final_newline = true
quote_type = single
trim_trailing_whitespace = true
